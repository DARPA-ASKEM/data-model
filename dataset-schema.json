{
    "title": "DatasetDocument",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string"
        },
        "description": {
            "title": "Description",
            "description": "Texual description of the dataset.",
            "type": "string"
        },
        "data_url": {
            "title": "Data Url",
            "description": "Url from which the dataset can be downloaded/fetched.",
            "minLength": 1,
            "maxLength": 65536,
            "format": "uri",
            "type": "string"
        },
        "columns": {
            "title": "Columns",
            "description": "Information regarding the columns that make up the dataset.",
            "type": "array",
            "items": {
                "$ref": "#/definitions/DatasetColumn"
            }
        },
        "metatada": {
            "title": "Metatada",
            "description": "(Optional) Unformatted metadata about the dataset.",
            "type": "object"
        },
        "source": {
            "title": "Source",
            "description": "Source of dataset",
            "type": "string"
        },
        "grounding": {
            "$ref": "#/definitions/Grounding"
        }
    },
    "required": [
        "name",
        "columns"
    ],
    "definitions": {
        "ColumnTypes": {
            "title": "ColumnTypes",
            "description": "An enumeration.",
            "enum": [
                "unknown",
                "boolean",
                "string",
                "char",
                "integer",
                "int",
                "float",
                "double",
                "timestamp",
                "datetime",
                "date",
                "time"
            ],
            "type": "string"
        },
        "Grounding": {
            "title": "Grounding",
            "type": "object",
            "properties": {
                "identifiers": {
                    "title": "Identifiers",
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                },
                "context": {
                    "title": "Context",
                    "type": "object"
                }
            },
            "required": [
                "identifiers"
            ]
        },
        "DatasetColumn": {
            "title": "DatasetColumn",
            "type": "object",
            "properties": {
                "name": {
                    "title": "Name",
                    "description": "Name of column",
                    "type": "string"
                },
                "data_type": {
                    "description": "Datatype. One of: unknown, boolean, string, char, integer, int, float, double, timestamp, datetime, date, time",
                    "default": "unknown",
                    "allOf": [
                        {
                            "$ref": "#/definitions/ColumnTypes"
                        }
                    ]
                },
                "format_str": {
                    "title": "Format Str",
                    "description": "String that describes the formatting of the value.",
                    "type": "string"
                },
                "annotations": {
                    "title": "Annotations",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "metadata": {
                    "title": "Metadata",
                    "type": "object"
                },
                "grounding": {
                    "$ref": "#/definitions/Grounding"
                }
            },
            "required": [
                "name",
                "annotations",
                "metadata"
            ]
        }
    }
}